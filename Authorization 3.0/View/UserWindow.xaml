<Window x:Class="Authorization_3._0.View.UserWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Authorization_3._0.View"
        mc:Ignorable="d"
        Title="UserWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="60"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="60"/>
        </Grid.RowDefinitions>

        <StackPanel Grid.Row="1" Margin="50,0,50,0" VerticalAlignment="Center">
            <TextBlock Text="Вы зашли в свой профиль" FontSize="40" HorizontalAlignment="Center"/>
            <TextBlock Text="Ожидайте" FontSize="30" HorizontalAlignment="Center"/>
        </StackPanel>
    </Grid>
</Window>
